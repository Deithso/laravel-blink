{
    "name": "spatie/laravel-blink",
    "description": "Cache that expires in the blink of an eye",
    "keywords": [
        "spatie",
        "laravel-blink"
    ],
    "homepage": "https://github.com/spatie/laravel-blink",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "spatie/blink": "dev-master",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelBlink\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelBlink\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
